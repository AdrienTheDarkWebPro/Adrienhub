local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Adrien's Universal Hub", "Synapse")

-- MAIN
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main Scripts")


MainSection:NewButton("Infinity Yield", "Infinity Yield FE", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)
MainSection:NewButton("Homebrew", "Homebrew", function()
    loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/Syntaxx64/HomebrewAdmin/master/Main"))()
end)
MainSection:NewButton("Fate's Admin", "Fate's Admin", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/fatesc/fates-admin/main/main.lua"))();
end)


-- BROOKHAVEN
local BROOKHAVEN = Window:NewTab("Brookhaven: RP")
local BROOKHAVENSection = BROOKHAVEN:NewSection("Brookhaven: RP Scripts")


BROOKHAVENSection:NewButton("Ice Hub", "Ice Hub Gui for Brookhaven", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/IceMael7/NewIceHub/main/Brookhaven"))()
end)
BROOKHAVENSection:NewButton("David Hub [PC]", "David Hub Gui for Brookhaven", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/DeividComSono/BrookhavenRoblox/main/loader.lua'))()
end)
BROOKHAVENSection:NewButton("David Hub [MOBILE]", "David Hub but on mobile for Brookhaven", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/DeividComSono/BrookhavenRoblox/main/mobile.lua'))()
end)
